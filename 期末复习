#include<iostream>
#include<math.h>
#include<string>
using namespace std;

int main(){
	int m;
	cin>>m;
	int A[m];
	for(int i=0;i<m;i++){
		cin>>A[i];
	}
	int x=A[0];
	int y=A[0];
	for(int i=0;i<m;i++){
		x=max(x,A[i]);
		y=min(y,A[i]);
	}
	//cout<<x;
	//cout<<y;
	int w=abs(x);
	int z=abs(y);
	if(w>=z){
		cout<<x;
	}
	else cout<<y;
	return 0;
}

