#include<iostream>
#include<stdio.h>
#include<algorithm>
using namespace std;
double xs[1000];

int main() {
	int xuanshou;
	cin >> xuanshou;
	int pingwei;
	cin >> pingwei;
	double res[1000];double resu = 0;
	double minnn;double maxxx;
	for (int i = 0;i < xuanshou;i++) {
		for (int j = 0;j < pingwei;j++) {
			cin >> xs[j];resu += xs[j];
		}
		minnn = xs[0];maxxx = xs[0];
		for(int j = 0;j < pingwei;j++){
			minnn = min(xs[j],minnn);
			maxxx = max(xs[j],maxxx);
		}
		res[i] = (resu - minnn - maxxx) / (pingwei - 2);
		resu = 0;
	}
	double maxx = res[0];
	for (int i = 0;i < xuanshou;i++) {
		maxx=max(res[i], maxx);
	}
	printf("%.2f", maxx);
	return 0;
} 
