 #include <stdio.h>
 int main()
 {
 	int h, j;
 	int num;
 	scanf("%d", &num);
	printf("\n", num);
 	
 	for (h = 1; h <= num ; h++){
 		for (j = 0; j < h; j++)
          printf("* ");
		  printf("\n");
		  	 }
		  	 return 0;
 }
