#include <iostream>
#include <string>
#include <cstring>
using namespace std;

string word[1000];//出现了的单词
int cnt[1000];//单词出现的次数
int pos;

int main(){
	string str;//整个文本 
	getline(cin,str);//输入（可包含空格）
	
	int j=0;
	char chuxian[100];
	string w;
	memset(chuxian,0,sizeof(chuxian));
	for(int i=0;i<str.size();i++){
		if(str[i]>='a'&&str[i]<='z'){
			chuxian[j++]=str[i];	
		}
		else if(str[i]>='A'&&str[i]<='Z'){
			chuxian[j++]=str[i]+32;
		} 
		else{
			w=chuxian;
			if(w.size()==0)continue;
			int ii;
			for(ii=0;ii<pos;ii++){
				if(w==word[ii]){
					cnt[ii]++;
					break;
				}
			}
			if(ii==pos){
				word[pos]=w;
				cnt[pos]=1;
				pos++;
			}
	
			j=0;
			memset(chuxian,0,sizeof(chuxian));
		}
		
	}
	
	for(int i=0;i<pos;i++){
		cout<<word[i]<<" "<<cnt[i]<<endl;
	}
	
	return 0; 
}
