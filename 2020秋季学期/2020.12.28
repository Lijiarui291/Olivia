//递归算法计算n的阶乘 
#include<stdio.h>
int jiecheng(int n){
	if(n==1){return 1;
	}
	else{return (jiecheng(n-1)*n);
	}
}

int main(){
	int n;
	scanf("%d", &n);
	int w=jiecheng(n);
	printf("%d", w);
	return 0;
}
