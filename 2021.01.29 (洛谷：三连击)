//洛谷 三连击
#include<iostream>
#include<algorithm>
using namespace std;
int res[9];

int main() {
	int a[9] = {1,2,3,4,5,6,7,8,9};
	//cout << a;
	int cntt = 0;
	for (int i = 123;i < 322;i++) {
		int ii = 2 * i;int iii = 3 * i;
		res[0] = i / 100 % 10;res[1] = i / 10 % 10;res[2] = i / 1 % 10;
		res[3] = ii / 100 % 10;res[4] = ii / 10 % 10;res[5] = ii / 1 % 10;
		res[6] = iii / 100 % 10;res[7] = iii / 10 % 10;res[8] = iii / 1 % 10;
		sort(res, res + 9);
		int cnt = 0;
		for (int j = 0;j < 9;j++) {
			if (res[j] == a[j]) {
				cnt++;
			}
		}
		if (cnt == 9) {
			cout << i << " ";
			cout << ii << " ";
			cout << iii << endl;
		}
	}

	return 0;
}
